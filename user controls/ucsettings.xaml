									Height="26"
									HorizontalAlignment="Right"
									VerticalAlignment="Top"
									Background="{DynamicResource ButtonBackgroundColor}"
									BorderBrush="{DynamicResource ButtonBackgroundColor}"
									Margin="0,0,19,0"
									Click="EditColor">
									<PackIcon
										PackIcon.Kind="Edit"
										FrameworkElement.Height="26"
										FrameworkElement.HorizontalAlignment="Center"
										FrameworkElement.VerticalAlignment="Center"
										Control.Foreground="{DynamicResource ButtonForegroundColor}" xmlns="clr-namespace:MaterialDesignThemes.Wpf;assembly=MaterialDesignThemes.Wpf" />
								</Button>
							</Grid>
						</StackPanel>
					</ScrollViewer>
				</Grid>
			</Grid>
		</Border>
		<Grid
			Name="pnlPopup"
			HorizontalAlignment="Left"
			Height="578"
			VerticalAlignment="Top"
			Width="585"
			Visibility="Hidden">
			<Grid
				Background="#FF040404"
				Opacity="0.5" />
			<Border
				Name="bColorEditor"
				Height="250"
				Width="500"
				HorizontalAlignment="Center"
				VerticalAlignment="Center"
				Background="{DynamicResource GridBackgroundColor}"
				CornerRadius="5">
				<StackPanel
					Orientation="Vertical"
					Margin="15">
					<Grid>
						<Border
							Height="30"
							Width="30"
							HorizontalAlignment="Left"
							Background="{DynamicResource IconBackgroundColor}"
							CornerRadius="4"
							BorderThickness="1">
							<PackIcon
								PackIcon.Kind="Eyedropper"
								FrameworkElement.Height="26"
								FrameworkElement.Width="24"
								FrameworkElement.HorizontalAlignment="Center"
								FrameworkElement.VerticalAlignment="Center"
								Control.Foreground="{DynamicResource IconForegroundColor}"
								UIElement.Opacity="0.9"
								FrameworkElement.Margin="2,1" xmlns="clr-namespace:MaterialDesignThemes.Wpf;assembly=MaterialDesignThemes.Wpf" />
						</Border>
						<Label
							Content="Color Editor"
							Height="35"
							Width="135"
							HorizontalAlignment="Left"
							HorizontalContentAlignment="Left"
							VerticalContentAlignment="Center"
							FontWeight="Bold"
							FontSize="16"
							Foreground="{DynamicResource LabelHeaderColor}"
							Margin="35,0,0,0" />
						<Button
							Height="35"
							Width="35"
							HorizontalAlignment="Right"
							Style="{StaticResource MaterialDesignIconButton}"
							Foreground="{DynamicResource PrimaryDataColor}"
							Click="ClosePopup">
							<ContentControl.Content>
								<wpf:PackIconExtension
									PackIconExtension.Kind="Close"
									PackIconExtension.Size="28" xmlns:wpf="clr-namespace:MaterialDesignThemes.Wpf;assembly=MaterialDesignThemes.Wpf" />
							</ContentControl.Content>
						</Button>
					</Grid>
					<StackPanel
						Orientation="Horizontal"
						HorizontalAlignment="Center"
						Margin="0,5">
						<ColorZone
							ColorZone.Name="colorZone"
							FrameworkElement.Height="26"
							FrameworkElement.Width="26"
							FrameworkElement.HorizontalAlignment="Left"
							Control.Background="#FFFFFFFF"
							ColorZone.CornerRadius="5"
							x:Name="colorZone" xmlns="clr-namespace:MaterialDesignThemes.Wpf;assembly=MaterialDesignThemes.Wpf" />
						<TextBox
							Name="tbCustomColor"
							Margin="7,0"
							Style="{DynamicResource TextBox}"
							p13:HintAssist.Hint="#FFF"
							Width="175"
							TextChanged="tbCustomColor_TextChanged" xmlns:p13="clr-namespace:MaterialDesignThemes.Wpf;assembly=MaterialDesignThemes.Wpf" />
						<Label
							Name="lblColorName"
							Content="Color Name"
							HorizontalAlignment="Left"
							FontWeight="Medium"
							FontSize="14"
							Foreground="#FFFFFFFF"
							Margin="5,0,0,0" />
					</StackPanel>
					<wpf:ColorPicker
						ColorPicker.Name="colorPicker"
						FrameworkElement.Height="150" xmlns:wpf="clr-namespace:MaterialDesignThemes.Wpf;assembly=MaterialDesignThemes.Wpf"
						x:Name="colorPicker"
						MouseUp="ChangeColor" />
				</StackPanel>
			</Border>
		</Grid>
	</Grid>
</controls:UserControl>
